{
  "name": "themzway",
  "displayName": "themzway",
  "publisher": "themzway",
  "description": "This is a VS Code extension that helps you practice programming questions.",
  "version": "3.2.3",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "themzway.createFiles",
        "title": "Create Files"
      },
      {
        "command": "themzway.practice",
        "title": "Practice Programming Questions"
      },
      {
        "command": "themzway.ama",
        "title": "Ask Me Anything"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
